/dts-v1/;

/* First 4KB has trampoline code for secondary cores. */
/memreserve/ 0x80000000 0x00010000;

#include "fsl-imx8dv.dtsi"

/ {
	model = "Freescale i.MX8dv Zebu";
	compatible = "fsl,imx8dv-zebu", "fsl,imx8dv";

	chosen {
		bootargs = "console=ttymxc2,115200 earlycon=imxuart,0x5a080000,115200";
		stdout-path = &uart3;
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_leds>;
		user {
			label = "debug";
			gpios = <&gpio0 4 0>;
			default-state = "on";
		};
	};
};

&iomuxc {
	imx8dv-zebu {
        /*pin flag mux config iso wakeup gp*/
    	pinctrl_fec1: fec1grp {
            fsl,pins = <
            420 5 0 0 0 0 0x70
            421 5 0 0 0 0 0x70
            428 5 0 0 0 0 0x70
            429 5 0 0 0 0 0x70
            430 5 0 0 0 0 0x70
            431 5 0 0 0 0 0x70
            432 5 0 0 0 0 0x70
            433 5 0 0 0 0 0x70

            422 5 0 0 0 0 0x10
            423 5 0 0 0 0 0x10
            424 5 0 0 0 0 0x10
            425 5 0 0 0 0 0x10
            426 5 0 0 0 0 0x10
			>;
		};

       pinctrl_usdhc1: usdhc1grp {
            fsl,pins = <
            0x191 5 0 3 0 0 0x10
            >;
       };
       pinctrl_usdhc2: usdhc2grp {
            fsl,pins = <
            409 5 0 3 0 0 0x4C
            410 5 0 3 0 0 0x4C
            411 5 0 3 0 0 0x4C
            412 5 0 3 0 0 0x4C
            413 5 0 3 0 0 0x4C
            414 5 0 3 0 0 0x4C
            415 5 0 3 0 0 0x4C
            416 5 0 3 0 0 0x4C
            417 5 0 3 0 0 0x4C
            418 5 0 3 0 0 0x4C
            419 5 0 3 0 0 0x4C
            >;
       };
		pinctrl_gpio_leds: gpioledsgrp {
			fsl,pins = <
			146 5 0 0 0 0 0x10
			>;
		};
	};
};

&usdhc1 {
	pinctrl-names = "default";
	bus-width = <1>;
	non-removable;
	no-1-8-v;
	status = "okay";
};

&usdhc2 {
	bus-width = <4>;
	non-removable;
	no-1-8-v;
	max-frequency = <25000000>;
	status = "okay";
};

&usbotg1 {
/*	vbus-supply = <&reg_usb_otg1_vbus>;*/
	srp-disable;
	hnp-disable;
	adp-disable;
	status = "okay";
};

&pwm1 {
	status = "okay";
};

&pwm2 {
	status = "okay";
};

&fec1 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_fec1>;
    phy-mode = "rgmii";

	phy-handle = <&ethphy0>;
	fsl,magic-packet;
	status = "okay";

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		ethphy0: ethernet-phy@0 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <0>;
		};

		ethphy1: ethernet-phy@1 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <1>;
		};
	};
};
