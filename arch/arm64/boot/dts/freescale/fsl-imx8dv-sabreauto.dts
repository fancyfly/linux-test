/dts-v1/;

/* First 4KB has trampoline code for secondary cores. */
/memreserve/ 0x80000000 0x00010000;

#include "fsl-imx8dv.dtsi"

/ {
	model = "Freescale i.MX8DV Sabreauto";
	compatible = "fsl,imx8dv-sabreauto", "fsl,imx8dv";

	chosen {
		bootargs = "console=ttymxc2,115200 earlycon=imxuart,0x5a080000,115200";
		stdout-path = &uart3;
	};

	leds {
		compatible = "gpio-leds";
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_gpio_leds>;
		user {
			label = "debug";
			gpios = <&gpio0 4 0>;
			default-state = "on";
		};
	};
};
&framebuffer0 {
	status = "okay";
};
&framebuffer1 {
	status = "disabled";
};
&framebuffer2 {
	status = "disabled";
};
&framebuffer3 {
	status = "disabled";
};
&imxdpu0 {
	status = "okay";
};
&imxdpu1 {
	status = "disabled";
};

&i2c0 {
	clock-frequency = <100000>;
	status = "okay";
};

&i2c1 {
	clock-frequency = <100000>;
	status = "okay";
};

&i2c2 {
	clock-frequency = <100000>;
	status = "okay";
};

&i2c3 {
	clock-frequency = <100000>;
	status = "okay";
};

&iomuxc {
	imx8dv-sabreauto {
		pinctrl_usdhc1: usdhc1grp {
			fsl,pins = <
			SC_P_USDHC0_CLK 5 0 3 0 0 0x4c
			SC_P_USDHC0_CMD 5 0 3 0 0 0x4c
			SC_P_USDHC0_DATA0 5 0 3 0 0 0x4c
			SC_P_USDHC0_DATA1 5 0 3 0 0 0x4c
			SC_P_USDHC0_DATA2 5 0 3 0 0 0x4c
			SC_P_USDHC0_DATA3 5 0 3 0 0 0x4c
			SC_P_USDHC0_DATA4 5 0 3 0 0 0x4c
			SC_P_USDHC0_DATA5 5 0 3 0 0 0x4c
			SC_P_USDHC0_DATA6 5 0 3 0 0 0x4c
			SC_P_USDHC0_DATA7 5 0 3 0 0 0x4c
			>;
		};

		pinctrl_qspi: qspi {
			fsl,pins = <
			SC_P_QSPI0A_DATA0 5 0 3 0 0 0x4c
			SC_P_QSPI0A_DATA1 5 0 3 0 0 0x4c
			SC_P_QSPI0A_DATA2 5 0 3 0 0 0x4c
			SC_P_QSPI0A_DATA3 5 0 3 0 0 0x4c
			SC_P_QSPI0A_DQS   5 0 3 0 0 0x4c
			SC_P_QSPI0A_SCLK  5 0 3 0 0 0x4c
			SC_P_QSPI0A_SS1   5 0 3 0 0 0x4c
			SC_P_QSPI0A_SS0   5 0 3 0 0 0x4c
			SC_P_QSPI0B_DATA0 5 0 3 0 0 0x4c
			SC_P_QSPI0B_DATA1 5 0 3 0 0 0x4c
			SC_P_QSPI0B_DATA2 5 0 3 0 0 0x4c
			SC_P_QSPI0B_DATA3 5 0 3 0 0 0x4c
			SC_P_QSPI0B_DQS   5 0 3 0 0 0x4c
			SC_P_QSPI0B_SCLK  5 0 3 0 0 0x4c
			SC_P_QSPI0B_SS0   5 0 3 0 0 0x4c
			>;
		};

        /*pin flag mux config iso wakeup gp*/
    	pinctrl_fec1: fec1grp {
            fsl,pins = <
            420 5 0 0 0 0 0x62
            421 5 0 0 0 0 0x62
            428 5 0 0 0 0 0x62
            429 5 0 0 0 0 0x62
            430 5 0 0 0 0 0x62
            431 5 0 0 0 0 0x62
            432 5 0 0 0 0 0x62
            433 5 0 0 0 0 0x62

            422 5 0 0 0 0 0x00
            423 5 0 0 0 0 0x00
            424 5 0 0 0 0 0x00
            425 5 0 0 0 0 0x00
            426 5 0 0 0 0 0x00
            427 5 0 0 0 0 0x00
			>;
		};

		pinctrl_i2c2: i2c2grp {
			fsl,pins = <
			SC_P_LSIO_I2C2_SDA 5 0 3 0 0 0x4c
			SC_P_LSIO_I2C2_SCL 5 0 3 0 0 0x4c
			>;
		};

        pinctrl_usdhc2: usdhc2grp {
            fsl,pins = <
            409 5 0 3 0 0 0x4C
            410 5 0 3 0 0 0x4C
            411 5 0 3 0 0 0x4C
            412 5 0 3 0 0 0x4C
            413 5 0 3 0 0 0x4C
            414 5 0 3 0 0 0x4C
            415 5 0 3 0 0 0x4C
            416 5 0 3 0 0 0x4C
            417 5 0 3 0 0 0x4C
            418 5 0 3 0 0 0x4C
            419 5 0 3 0 0 0x4C
            >;
       };
		pinctrl_gpio_leds: gpioledsgrp {
			fsl,pins = <
			146 5 0 0 0 0 0x10
			>;
		};
	};
};

&usdhc1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc1>;
	bus-width = <1>;
	non-removable;
	no-1-8-v;
	max-frequency = <25000000>;
	status = "okay";
};

&usdhc2 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_usdhc2>;
	bus-width = <4>;
	no-1-8-v;
	max-frequency = <25000000>;
	status = "okay";
};

&qspi1 {
	pinctrl-names = "default";
	pinctrl-0 = <&pinctrl_qspi>;
	status = "okay";
	ddrsmp=<0>;

	flash0: n25q256a@0 {
                compatible = "micron,n25q256a";
                spi-max-frequency = <29000000>;
                reg = <0>;
        };

        flash1: n25q256a@1 {
                compatible = "micron,n25q256a";
                spi-max-frequency = <29000000>;
                reg = <1>;
        };
};

&usbotg1 {
	srp-disable;
	hnp-disable;
	adp-disable;
	status = "okay";
};

&pwm1 {
	status = "okay";
};

&pwm2 {
	status = "okay";
};

&fec1 {
    pinctrl-names = "default";
    pinctrl-0 = <&pinctrl_fec1>;
    phy-mode = "rgmii";
    status = "okay";

	phy-handle = <&ethphy0>;
	fsl,magic-packet;
	status = "okay";

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		ethphy0: ethernet-phy@0 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <0>;
		};

		ethphy1: ethernet-phy@1 {
			compatible = "ethernet-phy-ieee802.3-c22";
			reg = <1>;
		};
	};
};
